                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT M.id_RegistroMatricula AS MATRICULA, ES.primerNombre + ' ' + ES.segundoNombre + ' ' + ES.primerApellido + ' ' + ES.segundoApellido AS 'Nombre Estudiante', DN.nota1erParcial AS PARCIAL1, DN.nota2doParcial AS PARCIAL2, 
                  DN.nota3erParcial AS PARCIAL3, DN.nota4toParcial AS PARCIAL4, DN.notaFinal
FROM     detalleNotas AS DN INNER JOIN
                  detalleMatricula AS DM ON DN.id_DetalleMatricula = DM.id_DetalleMatricula INNER JOIN
                  matricula AS M ON DM.id_RegistroMatricula = M.id_RegistroMatricula INNER JOIN
                  datosEstudiante AS ES ON M.id_Estudiante = ES.identidadEstudiante</CommandText>
