                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT matricula.id_RegistroMatricula, datosEstudiante.identidadEstudiante, datosEstudiante.primerNombre, datosEstudiante.primerApellido, datosEstudiante.fechaNacimiento, datosEstudiante.genero, cursos.nombreCurso, 
